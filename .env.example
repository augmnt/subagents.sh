# Environment
NODE_ENV=development

# Site Configuration
NEXT_PUBLIC_SITE_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=Subagents.sh
NEXT_PUBLIC_APP_DESCRIPTION=Discover and share AI subagents

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# GitHub Integration
GITHUB_TOKEN=your_github_token
GITHUB_WEBHOOK_SECRET=your_webhook_secret

# Feature Flags
NEXT_PUBLIC_MAINTENANCE_MODE=false

# Query Optimization Feature Flags
# Set to 'true' to enable optimized direct SDK queries
NEXT_PUBLIC_USE_OPTIMIZED_QUERIES=false
NEXT_PUBLIC_USE_OPTIMIZED_SEARCH=false
NEXT_PUBLIC_USE_OPTIMIZED_FACETS=false
NEXT_PUBLIC_USE_OPTIMIZED_CATEGORIES=false