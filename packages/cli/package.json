{
  "name": "subagents-sh",
  "version": "0.1.0",
  "description": "CLI for discovering and installing Claude Code subagents",
  "type": "module",
  "bin": {
    "subagents-sh": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "claude",
    "claude-code",
    "subagents",
    "ai",
    "agents",
    "cli"
  ],
  "author": "augmnt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/augmnt/subagents.sh.git",
    "directory": "packages/cli"
  },
  "homepage": "https://subagents.sh",
  "bugs": {
    "url": "https://github.com/augmnt/subagents.sh/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@clack/prompts": "^0.10.0",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^2.1.0",
    "typescript": "^5.0.0",
    "vitest": "^2.1.0"
  }
}
